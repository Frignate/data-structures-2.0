package movementsystem;

import java.util.ArrayList;

public class questDatabase {
	public static ArrayList<quest> questData = new ArrayList<>(null) ;
	
	public static void setupQuests()
	{
		questData.clear();
		questData.add(new quest(1,1001,1,"Check Manor Basement","Come on",2));
		questData.add(new quest(2,5,1,"Check on erina","Come on",3));
		questData.add(new quest(3,6,1, "Fight Erina","Kill her",0));
		
	}
}
